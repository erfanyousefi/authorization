                                </li>
                            </ul>
                        </nav>
                    </div>
                    <div class="registration">
                        <%if(req.isAuthenticated()){%>
                            <a href="/dashboard" class="register">Dashboard</a>
                            <a href="/logout" class="login">Logout</a>
                            <%}else{%>
                                <a href="/auth/login" class="login">Login</a>
                                <a href="/auth/register" class="register">Register</a>
                                <%}%>
                    </div>
                    <!-- mobile menu trigger -->
                    <div class="mobile-menu-trigger">
                        <span></span>
                    </div>
                </nav>
            </div>
        </header>
        <!-- navbar- -->
        <div class="hero-area-l-17 position-relative">
            <div class="container">
                <div class="row position-relative justify-content-center">
                    <div class="col-xxl-6 col-xl-7 col-lg-8 col-md-11 order-lg-1 order-1" data-aos="fade-right" data-aos-duration="800" data-aos-once="true">
                        <div class="content text-center">
                            <h1>Dinstagram Token</h1>
                            <p>The largest decentralized social network in the world</p>
                            <a href="#" class="btn byBtn">Buy Now</a>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <%-body%>

            <!--Footer Area-->
            <footer class="footer-area-l-17 position-relative pt-5">
