                    </div>
                </div>
            </footer>
    </div>
    <!-- Vendor Scripts -->
    <script src="/js/vendor.min.js"></script>
    <!-- Plugin's Scripts -->
    <script src="//cdn.jsdelivr.net/npm/sweetalert2@11"></script>
    <script src="/plugins/fancybox/jquery.fancybox.min.js"></script>
